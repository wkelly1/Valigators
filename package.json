{
  "name": "valigators",
  "version": "1.0.0",
  "description": "A simple validation library for any data type",
  "main": "Valigators.js",
  "scripts": {
    "dev": "tsc --watch",
    "test": "jest --coverage",
    "updateReadme": "documentation readme src/Valigators.ts --section=API --parse-extension ts",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts ."
  },
  "keywords": [
    "validation",
    "validators",
    "validate",
    "check",
    "aligator"
  ],
  "author": "Will Kelly",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^2.38.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
}
